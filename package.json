{
  "name": "vite-plugin-cdp-mcp",
  "version": "0.1.0",
  "description": "Vite plugin integrating Chrome DevTools Protocol (CDP) with Model Context Protocol (MCP).",
  "private": false,
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "vite-plugin",
    "vite",
    "cdp",
    "mcp",
    "chrome-devtools-protocol",
    "model-context-protocol"
  ],
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --outDir dist --clean",
    "lint": "biome lint .",
    "format": "biome format . --write",
    "check": "biome check .",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build && pnpm test"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.5",
    "chrome-remote-interface": "^0.33.3",
    "vite": "^7.1.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.3",
    "@types/node": "^24.3.1",
    "@types/node-fetch": "^2.6.13",
    "ajv": "^8.17.1",
    "tsup": "^8.1.0",
    "node-fetch": "^3.3.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  }
}
